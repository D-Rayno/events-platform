{
  "extends": "@adonisjs/tsconfig/tsconfig.client.json",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
      "~/*": ["./*"],
      "#ui": ["./components/ui"],
      "#ui/*": ["./components/ui/*"],
      "#shared": ["./components/shared"],
      "#shared/*": ["./components/shared/*"],
      "#layouts": ["./components/layouts"],
      "#layouts/*": ["./components/layouts/*"],
      "#hooks": ["./hooks"],
      "#hooks/*": ["./hooks/*"],
      "#services": ["./services"],
      "#services/*": ["./services/*"],
      "#stores": ["./stores"],
      "#stores/*": ["./stores/*"],
      "#lib": ["./lib"],
      "#lib/*": ["./lib/*"],
      "#types": ["./types"],
      "#types/*": ["./types/*"]
    }
  },
  "include": ["./**/*"],
  "exclude": ["node_modules"]
}